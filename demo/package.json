{
    "name": "react-pdf-viewer-webpack-typescript-demo",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "copyasset": "cp -rf ../assets/*.pdf dist",
        "typecheck": "tsc --noEmit",
        "dev": "rm -rf dist && mkdir dist && npm run copyasset && NODE_ENV=development webpack serve"
    },
    "dependencies": {
