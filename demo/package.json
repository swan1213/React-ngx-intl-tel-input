        "@react-pdf-viewer/full-screen": "2.6.0",
        "@react-pdf-viewer/highlight": "2.6.0",
        "@react-pdf-viewer/page-navigation": "2.6.0",
        "@react-pdf-viewer/open": "2.6.0",
        "@react-pdf-viewer/print": "2.6.0",
        "@react-pdf-viewer/scroll-mode": "2.6.0",
        "@react-pdf-viewer/search": "2.6.0",
        "@react-pdf-viewer/toolbar": "2.6.0",
        "@react-pdf-viewer/zoom": "2.6.0"
    },
    "devDependencies": {
        "@babel/core": "^7.14.6",
        "@babel/preset-env": "^7.14.5",
        "@babel/preset-react": "^7.14.5",
        "babel-loader": "^8.2.2",
        "css-loader": "^5.2.6",
